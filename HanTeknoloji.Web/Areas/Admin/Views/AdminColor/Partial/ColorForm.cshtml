
@model ColorVM

<div class="col-lg-12">
    <div class="form-group">
        @Html.LabelFor(x => x.Name)
        @Html.TextBoxFor(x => x.Name, new { @class = "form-control", required = "required" })
        @Html.ValidationMessageFor(x => x.Name)
    </div>
    <div class="form-group">
        @Html.LabelFor(x => x.BarcodeValue)
        @Html.TextBoxFor(x => x.BarcodeValue, new { @class = "form-control", required = "required" })
        @Html.ValidationMessageFor(x => x.BarcodeValue)
    </div>
    <input type="submit" class="btn btn-primary" name="name" value="Kayıt" />
</div>
