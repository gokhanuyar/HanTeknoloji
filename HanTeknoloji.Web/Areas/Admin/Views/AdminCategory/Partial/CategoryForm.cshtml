

@model CategoryVM

<div class="col-lg-12">
    <div class="form-group">
        @Html.LabelFor(x => x.CategoryName)
        @Html.TextBoxFor(x => x.CategoryName, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.CategoryName)
    </div>
    <div class="form-group">
        @Html.LabelFor(x => x.BarcodeValue)
        @Html.TextBoxFor(x => x.BarcodeValue, new { @class = "form-control" })
    </div>
    <input type="submit" class="btn btn-success" name="name" value="Kayıt" />
</div>
