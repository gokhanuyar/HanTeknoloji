
@{
    ViewBag.Title = "EditTechnical";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h3 class="page-header">Maliyet Düzenleme</h3>
    </div>
</div>

<div class="row">
    <div class="col-lg-6">
        @Html.Partial("/Areas/Admin/Views/Shared/Partial/MessagesPart.cshtml")
        <div class="panel panel-default">
            <div class="panel-heading">
                Maliyet Düzenleme Formu
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        @using (Html.BeginForm(null, null, FormMethod.Post, new { @enctype = "multipart/form-data" }))
                        {
                            @Html.Partial("Partial/editTechnicalServiceCost")
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

